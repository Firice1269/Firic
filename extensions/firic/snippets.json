{
	"class": {
		"prefix": ["class ..."],
		"body": [
			"class $1 {",
			"\t$2",
			"\tfunc __init(${3:self}) {",
			"\t\t$0",
			"\t}",
			"}"
		]
	},
	"class.inherits": {
		"prefix": ["class ... < ..."],
		"body": [
			"class $1 < $2 {",
			"\t$3",
			"\tfunc __init(${4:self}) {",
			"\t\t$0",
			"\t}",
			"}"
		]
	},
	"else": {
		"prefix": ["else"],
		"body": [
			"else {",
			"\t$0",
			"}"
		]
	},
	"elseif": {
		"prefix": ["elseif ..."],
		"body": [
			"elseif $1 {",
			"\t$0",
			"}"
		]
	},
	"enum": {
		"prefix": ["enum ..."],
		"body": [
			"enum $1 {",
			"\t$0",
			"}"
		]
	},
	"for": {
		"prefix": [
			"for ... in ...",
			"for i in ..."
		],
		"body": [
			"for ${1:i} in $2 {",
			"\t$0",
			"}"
		]
	},
	"for.range": {
		"prefix": [
			"for ... in range(...)",
			"for i in range(...)"
		],
		"body": [
			"for ${1:i} in range($2) {",
			"\t$0",
			"}"
		]
	},
	"func": {
		"prefix": ["func ...(...)"],
		"body": [
			"func $1($2) {",
			"\t$0",
			"}"
		]
	},
	"func.anonymous": {
		"prefix": ["func (...)"],
		"body": [
			"func ($1) {",
			"\t$0",
			"}"
		]
	},
	"if": {
		"prefix": ["if ..."],
		"body": [
			"if $1 {",
			"\t$0",
			"}"
		]
	},
	"while": {
		"prefix": ["while ..."],
		"body": [
			"while $1 {",
			"\t$0",
			"}"
		]
	}
}
