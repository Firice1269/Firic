{
	"Case": {
		"prefix": ["case"],
		"body": [
			"case $1:",
			"\t$0"
		]
	},
	"Class": {
		"prefix": ["class"],
		"body": [
			"class $1 {",
			"\t$2",
			"\tfunc __init(${3:self}) {",
			"\t\t$0",
			"\t}",
			"}"
		]
	},
	"Default": {
		"prefix": ["default"],
		"body": [
			"default:",
			"\t$0"
		]
	},
	"Enum": {
		"prefix": ["enum"],
		"body": [
			"enum $1 {",
			"\t$0",
			"}"
		]
	},
	"For Loop": {
		"prefix": ["for"],
		"body": [
			"for ${1:i} in ${2:range()} {",
			"\t$0",
			"}"
		]
	},
	"Function": {
		"prefix": ["func"],
		"body": [
			"func $1($2) {",
			"\t$0",
			"}"
		]
	},
	"If Statement": {
		"prefix": ["if"],
		"body": [
			"if $1 {",
			"\t$0",
			"}"
		]
	},
	"Switch Statement": {
		"prefix": ["switch"],
		"body": [
			"switch $1 {",
			"\t$0",
			"}"
		]
	},
	"While Loop": {
		"prefix": ["while"],
		"body": [
			"while $1 {",
			"\t$0",
			"}"
		]
	}
}
