{
	"class": {
		"prefix": ["class"],
		"body": [
			"class $1 {",
			"\t$2",
			"",
			"\tfunc __init($3) {",
			"\t\t$0",
			"\t}",
			"}"
		]
	},
	"else": {
		"prefix": ["else"],
		"body": [
			"else {",
			"\t$0",
			"}"
		]
	},
	"elseif": {
		"prefix": ["elseif"],
		"body": [
			"elseif $1 {",
			"\t$0",
			"}"
		]
	},
	"for loop": {
		"prefix": ["for"],
		"body": [
			"for $1 in $2 {",
			"\t$0",
			"}"
		]
	},
	"function": {
		"prefix": ["func"],
		"body": [
			"func $1($2) {",
			"\t$0",
			"}"
		]
	},
	"if": {
		"prefix": ["if"],
		"body": [
			"if $1 {",
			"\t$0",
			"}"
		]
	},
	"while loop": {
		"prefix": ["while"],
		"body": [
			"while $1 {",
			"\t$0",
			"}"
		]
	}
}
